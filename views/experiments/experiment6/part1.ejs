<body bgcolor="#000066"> 
   
    <div id="dialogoverlay"></div>
    <div id="dialogbox">
        <div>
            <div id="dialogboxhead"></div>
            <div id="dialogboxbody"></div>
            <div id="dialogboxfoot"></div>
        </div>
    </div>

    <div id="whole_body" style=" background-color:white;position:relative; margin-left:auto; margin-right:auto; top:0px;height:1050px; width:1160px;">

        <div style="font-size:30px; text-align:center;padding-right:100px; height:60px"> <b> BJT- CE INPUT CHARACTERISTICS </b></div>
        <div id="ckt_values1" style="position:relative; top:180px;left:760px; height:300px; width:400px">
            <p>
                <img src ="./images/bjtce.png" style="width:700px;height:420px;top:-30px;position:absolute;right:50px" />  
            <div class="example-content" style="position:absolute; top:10px;right:150px;left:200px">
                <link rel="stylesheet" type="text/css" href="./stylesheets/css_metergauge_ver1.css">
                <!------------------------------------Metergauge1(Voltmeter)base-emitter voltage-------------------------------------------------->
                <div id="chart1" class="plot" style="position:relative;width:200px; height:110px;top:100px;right:420px"></div>
                <script src="./Js/js_metergaugecein_ver1.js"></script>

                <!------------------------------------Metergauge2(Ammeter)base current----------------------------------------------------->
                <div id="chart2" class="plot" style="position:absolute;width:250px; height:110px;right:260px;top:0px"></div>

                <!------------------------------------Metergauge3(voltmeter)collector-emitter voltage------------------------------------------------------>
                <div id="chart3" class="plot" style="position:absolute;width:250px; height:110px;right:70px;top:80px"></div>
<!------------------------------------Metergauge3(ammeter)collector current------------------------------------------------------>
                <div id="chart4" class="plot" style="position:absolute;width:250px; height:110px;right:70px;top:-50px"></div>
                
                <p style="position:absolute;top:70px;right:435px;font-size:15px"><b>I<sub>B</sub></b></p>
                 <p style="position:absolute;top:180px;right:450px;font-size:15px"><b>R<sub>h1</sub></b></p>
                <p style="position:absolute;top:20px;right:240px;font-size:15px"><b>I<sub>C</sub></b></p>
                 <p style="position:absolute;top:170px;right:75px;font-size:15px"><b>R<sub>h2</sub></b></p>
                <p style="position:absolute;top:168px;right:410px;font-size:15px"><b>V<sub>BE</sub></b></p>
                <p style="position:absolute;top:155px;right:220px;font-size:15px"><b>V<sub>CE</sub></b></p>
                <p style="position:absolute;top:190px;right:-30px;font-size:15px"><b>V<sub>CC</sub></b></p>
                <p style="position:absolute;top:190px;right:550px;font-size:15px"><b>V<sub>BB</sub></b></p>
             
                

            </div>	

           <input type="number" value=""  id='voltbe'  min="0.3" max="1" step="0.01"  placeholder="Voltmeter"   style="background-color:#e6f5ff;position:absolute; top:193px;  right:480px; width:70px;cursor:pointer" readonly/>
            <input type="number" value=""  id='voltce'  min="0.3" max="1" step="0.01"  placeholder="Voltmeter"   style="background-color:#e6f5ff;position:absolute; top:175px;  right:295px; width:70px;cursor:pointer" readonly/>             
           <input type="text" value="" id='ampb' placeholder=" Ammeter"  style="background-color:#e6f5ff;position:absolute; top:90px;  right:495px; width:70px;" readonly/>
            <input type="text" value="" id='ampc' placeholder=" Ammeter"  style="background-color:#e6f5ff;position:absolute; top:45px;  right:300px; width:70px;" readonly/>
       
        </div>


        <div id="cntrwrap" style="position:absolute;border: 2px solid #054d2b; top:40px;width:380px;height:150px;right:300px;">
            <div id="cntrlr" style="position:absolute; background-color:#054d2b;   height:50px; top:0px; right:0px; width:380px;font-size: 16px;font-weight: bold;"><p style="text-align:center;padding-right:210px;color:white"><b>CONTROLS</b></p></div>
            <div id="slide" style=" position:absolute; top:70px; right:10px; width:360px;  border-radius: 10px;">
             
              R<sub>h1</sub>  <input type="range"  id="base_r" min="1" max="100" step="1" value="1" oninput="resbChange()" style="width:150px;cursor:pointer"> &nbsp; Ohms<br>
              <input type="text" value="" id='r_base' placeholder="Resistance"  style="background-color:#e6f5ff;position:absolute; right:45px; top:5px; width:70px;" readonly/> 
          
              R<sub>h2</sub>  <input type="range"  id="collector_r" min="1" max="100" step="1" value="1" oninput="rescChange()" style="width:150px;cursor:pointer"> &nbsp; Ohms<br>
                  <input type="text" value="" id='r_collector' placeholder="Resistance"   style="background-color:#e6f5ff;position:absolute; top:35px; right:45px;width:70px;" readonly/>
           
                <br>	
                <input type="button" id="add" value="Add to Table" onclick="tabled()" style="position:absolute; top:100px; width:100px;  right:270px;cursor:pointer "/>  
                <input type="button" id="plt" value="Plot" onclick="plot()" style="position:absolute; top:100px; width:100px; right:160px;cursor:pointer "/>  
                <input type="button" id="clr" value="Clear" onclick="clearfn()" style="position:absolute; top:100px; width:100px; right:50px; cursor:pointer "/>
            </div></div>
        <div id="msg" style="position:absolute;top:40px;right:40px;width:250px;height:150px;">
            <p id="demo" style="color:red;"></p>
            <p id="demos" style="color:green;"></p>

        </div>
        <div id="description" style="background-color: #054d2b;   position:absolute; top:40px; left:30px; width:354px;font-size: 16px;font-weight: bold;">
            <div class="dropdown">
                <p style="text-align:center;padding-right:150px;color:white"> INSTRUCTION</p>
                <div class="dropdown-content">
                    <ol type="1">
                         <li>Initially set rheostat R<sub>h1</sub> = 1 &#937; and rheostat R<sub>h2</sub> = 1 &#937; </li>
                        <li>Set the Collector-Emitter Voltage(V<sub>CE</sub>) to 1 V  by adjusting the rheostat  R<sub>h2</sub></li>
                        <li> Base Emitter Voltage(V<sub>BE</sub>) is varied  by adjusting the rheostat  R<sub>h1</sub>.</li>
                        <li>Note the reading of Base current(I<sub>B</sub>)in &#956; Ampere.</li>
                        <li>Click on 'Plot' to plot the I-V characteristics of Common-Emitter configuration. A graph is drawn with VBE along X-axis and IB along Y-axis.</li>
                        <li>Click on 'Clear' button to take another sets of readings</li> 
                        <li>Now set the Collector-Emitter Voltage(V<sub>CE</sub>) to 2 V, 3 V, 4 V</li>
                        
                        

                    </ol> 
                </div>
            </div>
        </div>


        <div id="tblwrap" style="position:absolute;border: 2px solid #054d2b; top:100px;width:350px;height:490px;left:30px">
            <div style="position:absolute; font-size:16px;background-color: #054d2b;  top:0px;left:0px; width:350px; height:50px;font-weight: bold;"><p style="text-align:center;padding-right:80px;color:white"> <b> EXPERIMENTAL TABLE</b></p>
            </div>
            <div id="table" style="background-color:#e6f5ff;position:absolute;top:50px;width:350px;height:440px; left:0px;overflow-y: auto;">

                <table id="mytable" border="1" >                    
                    <tr>

                        <th rowspan="2"> Serial No.</th>
                        <th colspan="2">Collector-Emitter Voltage <br><input type="text" id="tbvce1" value="" size="4" readonly>V</th>
                    </tr> 
                    <tr>
                        <th scope="col">Base-Emitter Voltage<br>V</th>
                        <th scope="col">Base Current(&#956;A)</th>
                    </tr>

                </table>

            </div></div>
        <div id="grpwrap" style="position:absolute;border: 2px solid #054d2b; top:650px;width:1050px;height:360px;left:20px">
            <div style="position:absolute; font-size:16px; top:0px;left:0px;  height:50px;background-color:#054d2b;  width:1050px;font-weight: bold;"><p style="text-align:center;padding-right:910px;color:white"> <b> GRAPH PLOT</b></p>
                <div class="tooltip" ><button id="calci"  onclick="printf()" style="position:absolute;padding-right:40px; cursor:pointer;top:13px;right:20px;width:5px;height:25px;border:none; background-color:#054d2b;">
                        <img src="./images/Print%20Filled.png" alt="Save icon" style="width:20px;height:20px;top:13px;"/></button> 
                    <span class="tooltiptext">Print</span></div>
            </div>
            <div id="chartContainer" style=" position:absolute;  top:50px;  left:0px; height:300px; width:1050px;  ">

            </div>
        </div>


      
    </div>
    
    <script src="./Js/bjtcein_ver1.js"></script>


